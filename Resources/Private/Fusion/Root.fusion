prototype(Page) {
    head.@process.compression {
        expression = ${Flownative.Compressor.compress(value)}
        @position = 'end 999999999'
        @if.isLive = ${node.context.live}
    }

    body.@process.compression {
        expression = ${Flownative.Compressor.compress(value)}
        @position = 'end 999999999'
        @if.isLive = ${node.context.live}
    }
}
